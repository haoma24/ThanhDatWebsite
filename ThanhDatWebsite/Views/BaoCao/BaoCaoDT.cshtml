@using ReportViewerForMvc;
@{
    ViewBag.Title = "BaoCaoDT";
    Layout = "~/Views/Shared/_Admin.cshtml";
}

<main>
    <div class="justify-content-center align-content-center h-auto overflow-hidden">
        <div class="mt-2 mb-2"><a href="@Url.Action("DoanhThu","BaoCao")" class="btn btn-outline-dark">Quay lại</a></div>
        <section class="h-auto overflow-hidden" aria-labelledby="gettingStartedTitle">
            @Html.ReportViewer(ViewBag.ReportViewer as Microsoft.Reporting.WebForms.ReportViewer, new { scrolling = "no", format = "PDF" })
        </section>
    </div>
</main>